{
  "name": "invertus/inrecaptcha",
  "description": "Module inrecaptcha allows you secure your live shop from spams and prevents from abuses.",
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "prepend-autoloader": false
  },
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "prestashop/php-dev-tools": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Invertus\\Inrecaptcha\\": "src/"
    },
    "classmap": [
      "inrecaptcha.php",
      "controllers"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Invertus\\Inrecaptcha\\Tests\\": "tests/"
    }
  },
  "author": "Invertus",
  "license": "AFL-3.0"
}
