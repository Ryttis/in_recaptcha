  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "prestashop/php-dev-tools": "^3.0",
    "friendsofphp/php-cs-fixer": "v2.19.3",
    "phpstan/phpstan": "1.9.14"
  },
  "autoload": {
    "classmap": [
      "psrecaptcha.php",
      "controllers",
      "classes"
    ]
  },
